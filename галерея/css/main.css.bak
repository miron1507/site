* {
	padding:0;
	margin:0;
	box-sizing:border-box;
}

@font-face {
	font-family:HARDBOR;
	src:url(../fonts/HARDBOR.ttf);
}

body {
	background-image:url(../img/back.jpg);
	background-color: #efefef;
	font-family:Roboto;
}
p {
	background: #fff;	
}

.test1 {
	background:red;
	padding:20px;
}

main{
	width:70%;
	padding:50px;
	margin:auto;
	background-color:#828282;
	display: flex;
	flex-wrap:wrap;
	justify-content:center;
	box-shadow: 0 0 30px 3px rgba(0,0,0,1);
}

.photo {					
	border:3px solid #9400d3;
	width:calc(50% - 40px);
	height:calc((35vw - 40px)/16*9);
	margin:20px;
	background-color:orange;
	background-position:center;
	background-size:cover;
	transition:all 0.5s ease;
} 

.photo:hover{
	transform: rotate(0deg) scale(1.1);
	box-shadow: 0 0 30px 3px rgba(0,0,0,0.5);
}

h1 {
	width:100%;
	text-align:center;
	font-size:48px;
}

